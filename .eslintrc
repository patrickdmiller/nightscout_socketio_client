{
  "parser": "@typescript-eslint/parser",
  "extends": [
      "airbnb",
      "plugin:prettier/recommended"
  ],
  "plugins": [
      "prettier",
      "react"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      "prettier/prettier": ["error"]
  }
}